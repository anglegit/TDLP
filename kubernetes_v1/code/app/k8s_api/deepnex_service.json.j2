{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "{{ params['service_name'] }}",
    "labels": {
      "name": "{{ params['service_name'] }}",
      "username": "{{ params['user_name'] }}"
    }
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "port": 8888
      }
    ],
    "selector": {
      "app": "{{ params['pod_name'] }}"
    }
  }
}